// Import Genkit and Google AI plugin
import { genkit } from 'genkit';
import { googleAI } from '@genkit-ai/googleai';

// Initialize Genkit with Google AI plugin and set default model
const ai = genkit({
  plugins: [googleAI()],
  model: googleAI.model('gemini-2.5-flash'), // Optional: set default model
});

// Generate text
async function generateJoke() {
  const llmResponse = await ai.generate('Tell me a joke.');
  console.log(llmResponse.text);
}

// Get embeddings
async function getEmbeddings(input: string) {
  const embeddings = await ai.embed({
    embedder: googleAI.embedder('text-embedding-004'),
    content: input,
  });
  console.log(embeddings);
}

// Example usage
generateJoke();
getEmbeddings('Hello world!');

